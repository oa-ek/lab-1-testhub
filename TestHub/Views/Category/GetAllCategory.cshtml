@model IEnumerable<TestHub.Core.Models.Category>

@{
    ViewData["Title"] = "All Categories";
    Layout = "_Layout";
}

<h1>@ViewData["Title"]</h1>

<table>
    <thead>
    <tr>
        <th>Category Name</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var category in Model)
    {
        <tr>
            <td>@category.Title</td>
        </tr>
    }
    </tbody>
</table>